{% extends 'layout.html.twig' %}

{% block content %}
    <h1>Command's valid</h1>
    {{ form_start(form, { 'class': "form-horizontal", 'method': 'post') }}
    <div class="form-group">
        {{ form_label(form.name) }}
        <div class="col-10">
             {{ form_widget(form.name) }}
        </div>
    </div>
        
    <div class="form-group">
        {{ form_label(form.adress) }}
        <div class="col-10">
             {{ form_widget(form.adress) }}
        </div>
    </div>
        
    <div class="form-group">
        {{ form_label(form.town) }}
        <div class="col-10">
             {{ form_widget(form.town) }}
        </div>
    </div>
        
    <div class="form-group">
        {{ form_label(form.zipcode) }}
        <div class="col-10">
             {{ form_widget(form.zipcode) }}
        </div>
    </div>
    {{ form_widget(form) }}
    <div>
        <a href=" {{ path('action_valid_command' )}}"><input type="submit" name="send" value="send"/>Send</a>
    </div>
    {{ form_end(form) }}
    
{% endblock %}

